<!DOCTYPE html>
<html lang="en">
   <%- include('./partials/head.ejs') %>
<body>
    <h1>POKEMONS</h1>

    <div class="container">

       <% for(let i = 0; i < data.length; i++) { %>

        <div class="pokemonItem">
            <p><%= data[i].name  %> </p>
            <a href="/pokemon/<%- i%> "><img src="<%= data[i].img  %>" alt="<%= data[i].name%> image"></a>
            <div class="controlers">
                <button>edit</button>
                <button>delete</button>
            </div>
        
        
        </div>

        <% } %> 

    </div>



</body>
</html>